# Trumpet Labs Trading System - Environment Variables Template
# Copy this to .env and fill in your values

# ===== REQUIRED FOR TELEGRAM AUDIT TRAIL =====
TELEGRAM_BOT_TOKEN=6123456789:ABCdefGHIjklMNOpqrsTUVwxyz123456789
TELEGRAM_CHAT_ID=-1001234567890

# ===== AI ANALYSIS (HIGHLY RECOMMENDED) =====
RISK_SCORING=on
GEMINI_API_KEY=your_gemini_api_key_here

# ===== WEBHOOK SECURITY (REQUIRED) =====
APIFY_WEBHOOK_SECRET=your_super_secret_webhook_key_12345
TRADE_PANEL_SECRET=your_super_secret_trading_key_67890

# ===== CROSS-MATCH DETECTION SETTINGS =====
# Time window for cross-account detection (seconds)
WINDOW_SEC=300
# Minimum accounts needed for cross-match alert
MIN_UNIQUE_ACCOUNTS=2

# ===== OPTIONAL NEWS SOURCES =====
NEWS_API_KEY=your_newsapi_key_here

# ===== TRADING SYSTEM CONFIGURATION =====
# Enable IBKR integration (false = preview mode only)
IBKR_ENABLED=false
IBKR_HOST=localhost
IBKR_PORT=7497
IBKR_CLIENT_ID=1
IBKR_PAPER_TRADING=true

# ===== CUSTOM KEYWORDS =====
# Additional keywords for detection (comma-separated)
CUSTOM_KEYWORDS=your,custom,keywords,here
MARKET_KEYWORDS=SPY,QQQ,VIX,DXY,TLT,GLD

# ===== SYSTEM SETTINGS =====
LOG_LEVEL=info
PORT=8080

# ===== DEPLOYMENT URLs (Auto-set by Railway) =====
# Your webhook endpoints will be:
# https://your-app-name.railway.app/web/alert
# https://your-app-name.railway.app/trade